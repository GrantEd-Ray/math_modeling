import matplotlib.pyplot as plt
import numpy as np

def parabola_plotter(a = 1, b = 1, c = 0, title = 'Parabola'):
  x = np.arange(-10, 10, 0.01)
  y = a * x ** 2 + b * x + c
  plt.plot(x, y)
  plt.title(title)
  plt.show()

def hyperbola_plotter(k = 1, title = 'Hyperbola'):
  x = np.arange(-10, 10, 0.5)
  y = k / x
  plt.plot(x, y)
  plt.title(title)
  plt.show()

s = input()
if s == 'H':
  hyperbola_plotter()
elif s == 'P':
  parabola_plotter()
